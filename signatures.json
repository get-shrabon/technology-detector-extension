{
	"cms": {
		"icon": "üìù",
		"technologies": {
			"WordPress": {
				"patterns": [
					{ "type": "meta", "name": "generator", "content": "WordPress" },
					{ "type": "script", "src": "wp-(?:content|includes)" },
					{ "type": "link", "href": "wp-(?:content|includes)" },
					{ "type": "html", "content": "wp-(?:content|includes)" },
					{ "type": "global", "name": "wp" }
				]
			},
			"Shopify": {
				"patterns": [
					{ "type": "meta", "name": "generator", "content": "Shopify" },
					{ "type": "script", "src": "cdn\\.shopify\\.com" },
					{ "type": "html", "content": "Shopify\\.shop" },
					{ "type": "global", "name": "Shopify" }
				]
			},
			"Drupal": {
				"patterns": [
					{ "type": "meta", "name": "generator", "content": "Drupal" },
					{ "type": "html", "content": "drupal" },
					{ "type": "script", "src": "/sites/(default|all)/(modules|themes)" },
					{ "type": "global", "name": "Drupal" }
				]
			},
			"Joomla": {
				"patterns": [
					{ "type": "meta", "name": "generator", "content": "Joomla" },
					{ "type": "script", "src": "/media/jui/" },
					{ "type": "global", "name": "Joomla" }
				]
			},
			"Wix": {
				"patterns": [
					{ "type": "meta", "name": "generator", "content": "Wix" },
					{ "type": "script", "src": "static\\.wixstatic\\.com" }
				]
			},
			"Squarespace": {
				"patterns": [
					{ "type": "meta", "name": "generator", "content": "Squarespace" },
					{ "type": "script", "src": "static1\\.squarespace\\.com" }
				]
			},
			"Webflow": {
				"patterns": [
					{ "type": "meta", "name": "generator", "content": "Webflow" },
					{ "type": "html", "content": "webflow" }
				]
			}
		}
	},
	"frameworks": {
		"icon": "‚öõÔ∏è",
		"technologies": {
			"React": {
				"patterns": [
					{ "type": "script", "src": "react" },
					{ "type": "global", "name": "React" },
					{ "type": "html", "content": "data-reactroot|data-reactid" }
				]
			},
			"Vue.js": {
				"patterns": [
					{ "type": "script", "src": "vue(\\.min)?\\.js" },
					{ "type": "global", "name": "Vue" },
					{ "type": "html", "content": "v-bind|v-on|v-if" }
				]
			},
			"Angular": {
				"patterns": [
					{ "type": "script", "src": "angular(\\.min)?\\.js" },
					{ "type": "global", "name": "angular" },
					{ "type": "html", "content": "ng-app|ng-controller|ng-bind" }
				]
			},
			"jQuery": {
				"patterns": [
					{ "type": "script", "src": "jquery" },
					{ "type": "global", "name": "jQuery" }
				]
			},
			"Next.js": {
				"patterns": [
					{ "type": "script", "src": "_next/" },
					{ "type": "global", "name": "__NEXT_DATA__" },
					{ "type": "meta", "name": "next-head-count", "content": ".*" }
				]
			},
			"Nuxt.js": {
				"patterns": [
					{ "type": "script", "src": "_nuxt/" },
					{ "type": "global", "name": "__NUXT__" }
				]
			},
			"Svelte": {
				"patterns": [
					{ "type": "html", "content": "svelte-" },
					{ "type": "global", "name": "svelte" }
				]
			},
			"Ember.js": {
				"patterns": [
					{ "type": "script", "src": "ember" },
					{ "type": "global", "name": "Ember" }
				]
			}
		}
	},
